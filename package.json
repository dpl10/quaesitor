{
	"name": "quaesitor",
	"version": "1.0.7",
	"description": "a utility for finding Latin scientific within vernacular text",
	"main": "dist/quaesitor-node.js",
	"browser": "dist/quaesitor-browser.js",
	"types": "dist/src/index.d.ts",
	"scripts": {
		"build": "webpack --config webpack.config.js --mode production",
		"build.all": "npm run build.prod && npm run build.esnext",
		"build.esnext": "npm run build:types && npm run build:js",
		"build.prod": "webpack --config webpack.config.js -p",
		"build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
		"build:types": "tsc --emitDeclarationOnly",
		"lint": "tslint -p tsconfig.json",
		"start": "webpack-dev-server --port 9000 --inline --progress --profile --colors --watch --content-base src/ --mode development",
		"test": "karma start karma.config.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dpl10/quaesitor.git"
	},
	"author": "Damon P. Little",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/dpl10/quaesitor/issues"
	},
	"homepage": "https://github.com/dpl10/quaesitor#readme",
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.8.7",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.8.3",
		"@babel/preset-env": "^7.8.7",
		"@babel/preset-typescript": "^7.8.3",
		"@types/jasmine": "^3.5.9",
		"@types/lodash": "^4.14.149",
		"@types/node": "^13.9.0",
		"@types/pbf": "^3.0.2",
		"@types/xregexp": "^3.0.30",
		"awesome-typescript-loader": "5.2.1",
		"copy-webpack-plugin": "^5.1.1",
		"jasmine-core": "~3.5.0",
		"karma": "^4.4.1",
		"karma-jasmine": "^3.1.1",
		"karma-phantomjs-launcher": "1.0.4",
		"karma-webpack": "4.0.2",
		"source-map-loader": "0.2.4",
		"tslint": "^6.1.0",
		"tslint-loader": "3.6.0",
		"typescript": "^3.8.3",
		"webpack": "^4.42.0",
		"webpack-cli": "3.3.11",
		"webpack-dev-server": "^3.10.3",
		"webpack-node-externals": "^1.7.2",
		"webpack-preprocessor-loader": "git+https://github.com/Ramzeus/webpack-preprocessor-loader.git"
	},
	"files": [
		"dist/*"
	],
	"dependencies": {
		"@tensorflow/tfjs": "^1.7.0",
		"@tensorflow/tfjs-node": "^1.7.0",
		"downsample": "^1.0.3",
		"lodash": "^4.17.15",
		"mem": "^6.0.1",
		"mnemonist": "^0.32.0",
		"pbf": "^3.2.1",
		"xregexp": "^4.3.0",
		"xxhash-wasm": "^0.3.1"
	}
}
